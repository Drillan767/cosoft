{
  "name": "cosoft",
  "version": "1.0.0",
  "description": "CLI tool to interact with the CoSoft API, and more easily manage meeting rooms reservations.",
  "type": "module",
  "bin": {
    "cosoft": "./src/cli.js"
  },
  "scripts": {
    "start": "node src/cli.js",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "mcp-server": "node src/cli.js mcp-server",
    "bundle": "esbuild src/cli.js --bundle --platform=node --target=node18 --format=cjs --outfile=dist/cosoft-bundle.js --external:fsevents",
    "build": "npm run bundle && pkg dist/cosoft-bundle.js --targets node18-macos-arm64 --output dist/cosoft",
    "build:all": "npm run bundle && pkg dist/cosoft-bundle.js --targets node18-macos-x64,node18-macos-arm64,node18-linux-x64,node18-win-x64 --out-path dist/"
  },
  "keywords": [
    "cli",
    "meeting-rooms",
    "reservations"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@inquirer/confirm": "^5.1.16",
    "@inquirer/input": "^4.2.2",
    "@inquirer/password": "^4.0.21",
    "@inquirer/select": "^4.3.2",
    "axios": "^1.11.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "esbuild": "^0.25.12",
    "eslint": "^9.35.0",
    "pkg": "^5.8.1",
    "prettier": "^3.6.2"
  },
  "pkg": {
    "scripts": [
      "src/**/*.js"
    ],
    "assets": [
      ".env"
    ],
    "outputPath": "dist"
  }
}
